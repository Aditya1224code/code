#include<stdio.h>
int main(){
    int a[20]={1,2,3,4,5,6,7};
    int i,ele,t;
    printf("enter the element to search");
    scanf("%d",&ele);
    for(i=0;i<=6;i++){
        if (a[i]==ele){
            t=ele;
            break;
        }
    }
    if(t==ele){
        printf("element found");
        printf("\nposition is %d",i);
    }
    else{
      printf("element not found");
    }
    
    return 0;
}
